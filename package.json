{
  "name": "jstest",
  "version": "1.0.0",
  "description": "Test js file using Mocha and Chai",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel file.js --out-file file_compiled.js && prettier --no-semi *_compiled.js --write ",
    "appbuild": "babel src --out-dir dist && prettier --write ./dist/*.js",
    "format": "prettier --no-semi *.js --write",
    "start": "node index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^7.0.12",
    "babel-cli": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.42.0"
  }
}
